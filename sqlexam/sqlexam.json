sql = ' [{"q1": "What does SQL stand for?","chooseA1": "Strong Question Language","chooseA2": " Structured Query Language","chooseA3": " Structured Question Language","chooseA4": "String Question Language"},{"q2": "Which SQL statement is used to extract data from a database?","chooseB1": "EXTRACT","chooseB2": "GET","chooseB3": "SELECT","chooseB4": "OPEN"},{"q3": "Which SQL statement is used to update data in a database?","chooseC1": "SAVE AS","chooseC2": "SAVE","chooseC3": "UPDATE","chooseC4": "MODIFY"},{"q4": "Which SQL statement is used to delete data from a database?","chooseD1": "REMOVE","chooseD2": "DELETE","chooseD3": "COLLAPSE","chooseD4": "POP"},{"q5": "Which SQL statement is used to insert new data in a database?","chooseE1": "INSERT INTO","chooseE2": "ADD RECORD","chooseE3": "INSERT NEW","chooseE4": "ADD NEW"},{"q6": "With SQL, how do you select a column named FirstName from a table named Persons?","chooseF1": "SELECT FirstName FROM Persons","chooseF2": "EXTRACT FirstName FROM Persons","chooseF3": "SELECT Persons.FirstName","chooseF4": "SELECT from Persons FirstName"},{"q7": "With SQL, how do you select all the columns from a table named Persons??","chooseG1": "SELECT *.Persons","chooseG2": "SELECT [all] FROM Persons","chooseG3": "SELECT * FROM Persons","chooseG4": "SELECT Persons"},{"q8": "With SQL, how do you select all the records from a table named Persons where the value of the column FirstName is Peter?","chooseH1": "SELECT * FROM Persons WHERE FirstName=Peter","chooseH2": "SELECT [all] FROM Persons WHERE FirstName=Peter","chooseH3": "SELECT * FROM Persons WHERE FirstName<>Peter","chooseH4": "SELECT [all] FROM Persons WHERE FirstName LIKE Peter"},{"q9": "Which SQL statement is used to return only different values?","chooseI1": "SELECT UNIQUE","chooseI2": "SELECT DIFFERENT","chooseI3": "SELECT DISTINCT","chooseI4": "SELECT Changes"},{"q10": "Which SQL keyword is used to sort the result-set?","chooseJ1": " SORT BY","chooseJ2":"ORDER","chooseJ3": "ORDER BY","chooseJ4": " SORT"}]';